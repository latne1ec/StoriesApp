-(void)preloadVideoPlayer {

if (videoCount == self.videoArray.count) {

} else {

if (videoCount % 2 == 0) {
//Even

if (videoCount > self.videoArray.count) {

} else {
NSString *nextUrlString = [self.videoArray objectAtIndex:videoCount];
